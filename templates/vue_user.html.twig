<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{% block title %}Kartcentrum!{% endblock %}</title>
    {% block stylesheets %}
        {{ encore_entry_link_tags('user') }}
    {% endblock %}
    {% block javascripts %}
        {{ encore_entry_script_tags('user') }}
    {% endblock %}
    <script>window.user = '{{ app.user.username }}'</script>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
</head>
<body>
    <div id="app"></div>
</body>
</html>
